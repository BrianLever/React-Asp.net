                            </Grid.ColumnDefinitions>
                            <TextBlock TextAlignment="Left" FontSize="36" VerticalAlignment="Center" HorizontalAlignment="Left"
												Text="Next" Foreground="#FFFFFFFF" FontWeight="SemiBold">
                            </TextBlock>
                            <Grid Grid.Column="1" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
                                <Path Width="32.1368" Height="32.1355" HorizontalAlignment="Left" Stretch="Fill" Fill="#FFFFFFFF" 
													Data="F1 M 30.582,16.0677L 29.0273,16.0677C 29.0261,19.6511 27.5794,22.8802 25.2305,25.2318C 22.8816,27.5781 19.6523,29.026 16.069,29.026C 12.4857,29.026 9.25519,27.5781 6.90485,25.2318C 4.55731,22.8802 3.1106,19.6511 3.1106,16.0677C 3.1106,12.4844 4.55731,9.25519 6.90485,6.90375C 9.25519,4.55737 12.4857,3.10938 16.069,3.10938C 19.6523,3.10938 22.8816,4.55737 25.2305,6.90375C 27.5794,9.25519 29.0261,12.4844 29.0273,16.0677L 30.582,16.0677L 32.1368,16.0677C 32.1354,7.19269 24.9439,0 16.069,0C 7.19281,0 0.00128174,7.19269 0,16.0677C 0.00128174,24.9427 7.19281,32.1355 16.069,32.1355C 24.9439,32.1355 32.1354,24.9427 32.1368,16.0677L 30.582,16.0677 Z "/>
                                <Path Width="19.875" Height="3.92188" Stretch="Fill" Fill="#FFFFFFFF" HorizontalAlignment="Left" 
													Data="F1 M 0,8.61469L 19.875,8.61469L 19.875,4.69281L 0,4.69281"/>
                                <Path Width="12.2981" Height="13.3048" Grid.Column="1" Stretch="Fill" Fill="#FFFFFFFF" 
													Data="F1 M 12.2955,13.3048L 18.9505,6.65509L 12.2955,0L 17.9414,0L 24.5936,6.65509L 17.9414,13.3048L 12.2955,13.3048 Z " />
                            </Grid>
                        </Grid>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter TargetName="ExternalPath" Property="Style" Value="{StaticResource OrangeExternalPath}" ></Setter>
                            <Setter TargetName="InternalPath" Property="Style" Value="{StaticResource OrangeInternalPath}" ></Setter>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="root" Property="Opacity" Value="0.4" ></Setter>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

       
    <!-- End Buttons-->

    <!-- Black Border under keyboard and other screens-->
    <Style TargetType="Border" x:Key="blackBorderLine">
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="HorizontalAlignment" Value="Stretch" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Opacity" Value="1" />
    </Style>
    <Style TargetType="Border" x:Key="blackBorderLineFixed" BasedOn="{StaticResource blackBorderLine}">
        <Setter Property="Height" Value="260" />
    </Style>
</ResourceDictionary>