<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Default styles-->
    <Style TargetType="TextBlock">
        <Setter Property="FontFamily" Value="../Fonts/#Proxima Nova" />
        <Setter Property="Foreground" Value="#323232" />
    </Style>

    <Style TargetType="TextBox">
        <Setter Property="FontFamily" Value="../Fonts/#Proxima Nova" />
    </Style>


    <!--Full keyboard-->
    <Style TargetType="Grid" x:Key="FullKeyboard">
        <Setter Property="Margin" Value="0,15,0,0" />
        <Setter Property="VerticalAlignment" Value="Top" />
    </Style>

    <!--Symbol keyboard-->
    <Style TargetType="Grid" x:Key="SymbolKeyboard">
        <Setter Property="Margin" Value="0,30,0,0" />
        <Setter Property="VerticalAlignment" Value="Top" />
    </Style>


   

    <!-- Key button -->




    <Style TargetType="Border" x:Key="keyButtonRootConainer">
        <Setter Property="Background" Value="#ff01396d" />
        <!--<Setter Property="Background" Value="#FF323232" />-->
    </Style>
    <Style TargetType="Border" x:Key="keyButtonRootConainerSelected">
        <Setter Property="Background" Value="#ff053247" />
    </Style>
    <Style TargetType="Border" x:Key="keyButtonRootConainerDisabled">
        <Setter Property="Background" Value="#FF535353" />
    </Style>

    <Style TargetType="Border" x:Key="yellow_bottom_line" >
        <Setter Property="BorderBrush" Value="#E7BE5D" />
        <Setter Property="BorderThickness" Value="0,0,0,6.3" />
    </Style>


    <Style TargetType="Image" x:Key="keyButtonImage">
        <Setter Property="Width" Value="Auto" />
        <Setter Property="Height" Value="Auto" />
        <Setter Property="Margin" Value="19" />
        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="VerticalAlignment" Value="Center" />
    </Style>
    <Style TargetType="Image" x:Key="keyButtonImageSmallArrow">
        <Setter Property="Width" Value="Auto" />
        <Setter Property="Height" Value="Auto" />
        <Setter Property="Margin" Value="33" />
        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="VerticalAlignment" Value="Center" />
    </Style>

   


    <!-- End Keyboard -->



    


   


    <Style TargetType="UserControl" x:Key="Backspace">
        <Setter Property="Margin" Value="15,0,0,0" />
        <Setter Property="Width" Value="Auto" />
        <Setter Property="Height" Value="Auto" />
    </Style>


    <!--Error Section-->

    <Style TargetType="TextBlock" x:Key="ErrorLabel" BasedOn="{StaticResource {x:Type TextBlock}}">
        <Setter Property="FontSize" Value="28pt" />
        <Setter Property="Foreground" Value="Red"/>
        <Setter Property="TextWrapping" Value="Wrap"/>
        <Setter Property="Margin" Value="0" />
    </Style>

    <Style TargetType="FrameworkElement" x:Key="ErrorPlaceHolder">
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Bottom" />
    </Style>

    <!--End Error Section-->

    <Style TargetType="Button" x:Key="NextOnUserDetailsScreen">
        <Setter Property="Width" Value="120"/>
        <Setter Property="IsEnabled" Value="false"/>
        <Setter Property="Height" Value="55"/>
        <Setter Property="VerticalAlignment" Value="Bottom" />
        <Setter Property="HorizontalAlignment" Value="Right"/>
        <Setter Property="Margin" Value="0,0,10,10" />
    </Style>



    <!-- Screens -->



    <!-- End Screens -->



    <!-- Path -->

    <Style x:Key="BlueExternalPath" TargetType="Path">
        <Setter Property="Fill">
            <Setter.Value>
                <LinearGradientBrush StartPoint="0.5,1" EndPoint="0.499999,-1.5455e-006">
                    <GradientStop Color="#FF004587" Offset="0"/>
                    <GradientStop Color="#FF4AA3CD" Offset="1"/>
                </LinearGradientBrush>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="OrangeExternalPath" TargetType="Path">
        <Setter Property="Fill">
            <Setter.Value>
                <LinearGradientBrush StartPoint="0.5,1" EndPoint="0.5,-1.5455e-006">
                    <LinearGradientBrush.GradientStops>
                        <GradientStop Color="#FFFFBA78" Offset="0"/>
                        <GradientStop Color="#FFB55C32" Offset="1"/>
                    </LinearGradientBrush.GradientStops>
                </LinearGradientBrush>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="BlueInternalPath" TargetType="Path">
        <Setter Property="Fill">
            <Setter.Value>
                <LinearGradientBrush StartPoint="0.500004,1" EndPoint="0.500004,0">
                    <GradientStop Color="#FF4AA3CD" Offset="0"/>
                    <GradientStop Color="#FF004587" Offset="1"/>
                </LinearGradientBrush>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="OrangeInternalPath" TargetType="Path">
        <Setter Property="Fill">
            <Setter.Value>
                <LinearGradientBrush StartPoint="0.500004,1" EndPoint="0.500004,8.44244e-007">
                    <LinearGradientBrush.GradientStops>
                        <GradientStop Color="#FFB55C32" Offset="0"/>
                        <GradientStop Color="#FFFFBA78" Offset="1"/>
                    </LinearGradientBrush.GradientStops>
                </LinearGradientBrush>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="DisabledExternalPath" TargetType="Path">
        <Setter Property="Fill">
            <Setter.Value>
                <LinearGradientBrush StartPoint="0.500004,1" EndPoint="0.500004,6.08543e-006">
                    <GradientStop Color="#FFFFBA78" Offset="0"/>
                    <GradientStop Color="#6749A3CC" Offset="1"/>
                </LinearGradientBrush>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="DisabledInternalPath" TargetType="Path">
        <Setter Property="Fill">
            <Setter.Value>
                <LinearGradientBrush StartPoint="0.500004,1" EndPoint="0.500004,6.08543e-006">
                    <GradientStop Color="#6749A3CC" Offset="0"/>
                    <GradientStop Color="#67004487" Offset="1"/>
                </LinearGradientBrush>
            </Setter.Value>
        </Setter>
    </Style>



    <Style x:Key="BlueActiveRectangle" TargetType="Rectangle">
        <Setter Property="Fill">
            <Setter.Value>
                <SolidColorBrush Color="#FF1E97E0"></SolidColorBrush>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="DarkPressedRectangle" TargetType="Rectangle">
        <Setter Property="Fill">
            <Setter.Value>
                <SolidColorBrush Color="#FF323232"></SolidColorBrush>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="YellowActiveRectangle" TargetType="Rectangle">
        <Setter Property="Fill">
            <Setter.Value>
                <SolidColorBrush Color="#FFFFC107"></SolidColorBrush>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="YellowActivePath" TargetType="Path">
        <Setter Property="Fill">
            <Setter.Value>
                <SolidColorBrush Color="#FFFFC107"></SolidColorBrush>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="DarkActivePath" TargetType="Path">
        <Setter Property="Fill">
            <Setter.Value>
                <SolidColorBrush Color="#FF323232"></SolidColorBrush>
            </Setter.Value>
        </Setter>
    </Style>



    <Style x:Key="NextPressedRectangle" TargetType="Rectangle">
        <Setter Property="Fill">
            <Setter.Value>
                <SolidColorBrush Color="#FF323232"></SolidColorBrush>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- end Path -->

    <!-- TextBlock -->
    <Style TargetType="TextBlock" x:Key="FormLabel" BasedOn="{StaticResource {x:Type TextBlock}}">
        <Setter Property="Foreground" Value="#FF323232" />
        <Setter Property="FontSize" Value="54px" />
        <Setter Property="FontWeight" Value="SemiBold" />
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="TextAlignment" Value="Center"/>
        <Setter Property="Margin" Value="0,250,0,30" />
    </Style>
    <Style TargetType="TextBlock" x:Key="DarkLabel" BasedOn="{StaticResource {x:Type TextBlock}}">
        <Setter Property="Foreground" Value="#FF323232" />
        <Setter Property="FontWeight" Value="SemiBold" />
    </Style>
    <Style TargetType="TextBlock" x:Key="YellowLabel" BasedOn="{StaticResource {x:Type TextBlock}}">
        <Setter Property="Foreground" Value="#FFFFC107" />
        <Setter Property="FontWeight" Value="SemiBold" />
    </Style>

    <Style TargetType="TextBlock" x:Key="PleaseWaitEhrMessage" BasedOn="{StaticResource {x:Type TextBlock}}">
        <Setter Property="Foreground" Value="#FF323232" />
        <Setter Property="FontSize" Value="54px" />
        <Setter Property="FontWeight" Value="SemiBold" />
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="TextAlignment" Value="Center"/>
        <Setter Property="Margin" Value="0,250,0,30" />
    </Style>


    <Style TargetType="Run" x:Key="FormLabelEmpasis">
        <Setter Property="FontWeight" Value="Black" />
    </Style>
    <Style TargetType="Run" x:Key="FormLabelEmpasisEhrNotFound" BasedOn="{StaticResource FormLabelEmpasis}">
        <!--<Setter Property="TextDecorations" Value="Underline" />-->
    </Style>
    <Style TargetType="Run" x:Key="HealthRecordNotFound">
        <Setter Property="Foreground" Value="#a52222" />
    </Style>





    <!-- End TextBlock-->
    <!-- Buttons -->

