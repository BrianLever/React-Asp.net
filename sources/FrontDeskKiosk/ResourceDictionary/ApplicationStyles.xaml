        <Setter Property="HorizontalAlignment" Value="Right"/>
        <Setter Property="Margin" Value="0,0,25,130" />

        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">
                    <Grid  Width="151" Height="79" x:Name="root">
                        <Path Width="151" Height="79" x:Name="ExternalPath" Stretch="Fill" Style="{StaticResource BlueExternalPath}"
																	Data="F1 M 151.065,67.7017C 151.065,73.9336 145.358,78.9843 138.323,78.9843L 12.7383,78.9843C 5.7005,78.9843 0,73.9336 0,67.7017L 0,11.2825C 0,5.05066 5.7005,0 12.7383,0L 138.323,0C 145.358,0 151.065,5.05066 151.065,11.2825L 151.065,67.7017 Z ">
                        </Path>
                        <Path Width="143" Height="72" x:Name="InternalPath" Stretch="Fill"  Style="{StaticResource BlueInternalPath}"
																	Data="F1 M 145.453,62.1523C 145.453,68.8255 140.042,74.2369 133.369,74.2369L 17.6953,74.2369C 11.0183,74.2369 5.61066,68.8255 5.61066,62.1523L 3.88281,14.0273C 3.88281,7.35278 9.29297,1.94135 15.9675,1.94135L 135.095,1.94135C 141.768,1.94135 147.181,7.35278 147.181,14.0273L 145.453,62.1523 Z ">
                        </Path>
                        <Grid VerticalAlignment="Center" Width="120.491" Height="32.1355">
